\n        ...state,\n        loadingPage: false,\n      };\n    case GET_CURRENT_PAGE:\n      return {\n        ...state,\n        currentPage: payload,\n      };\n    default:\n      return {\n        ...state,\n      };\n  }\n}\n","/Users/lhestermonroyo/Desktop/my-projects/devly-app/client/src/reducers/authReducer.js",["300"],"import {\n  AUTH_BEGIN,\n  AUTH_END,\n  AUTH_SUCCESS,\n  AUTH_FAIL,\n  USER_LOADED,\n  AUTH_FORM_BEGIN,\n  AUTH_FORM_END,\n} from '../constants/authConstants';\n\nconst initialState = {\n  loading: false,\n  loadingForm: false,\n  token: localStorage.getItem('token'),\n  isAuthenticated: null,\n  userDetails: null,\n};\n\nexport default function (state = initialState, action) {\n  const { type, payload } = action;\n  switch (type) {\n    case AUTH_BEGIN:\n      return {\n        ...state,\n        loading: true,\n      };\n    case AUTH_END:\n      return {\n        ...state,\n        loading: false,\n      };\n    case AUTH_FORM_BEGIN:\n      return {\n        ...state,\n        loadingForm: true,\n      };\n    case AUTH_FORM_END:\n      return {\n        ...state,\n        loadingForm: false,\n      };\n    case USER_LOADED:\n      return {\n        ...state,\n        isAuthenticated: true,\n        userDetails: payload.userData,\n      };\n    case AUTH_SUCCESS:\n      localStorage.setItem('token', payload.userToken);\n      return {\n        ...state,\n        isAuthenticated: true,\n        userDetails: payload.userDetails,\n      };\n    case AUTH_FAIL:\n      localStorage.removeItem('token');\n      return {\n        ...state,\n        token: null,\n        isAuthenticated: false,\n      };\n    default:\n      return {\n        ...state,\n      };\n  }\n}\n","/Users/lhestermonroyo/Desktop/my-projects/devly-app/client/src/reducers/profileReducer.js",["301"],"/Users/lhestermonroyo/Desktop/my-projects/devly-app/client/src/reducers/postReducer.js",["302"],"/Users/lhestermonroyo/Desktop/my-projects/devly-app/client/src/components/Loading